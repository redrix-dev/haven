{
  "name": "haven",
  "productName": "Haven",
  "version": "1.2.0",
  "description": "Haven",
  "main": ".webpack/main",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/redrix-dev/haven.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "test:unit": "vitest run src/components/__tests__ src/lib/notifications/__tests__ src/main/__tests__",
    "test:backend": "node scripts/test/run-vitest-with-local-env.mjs backend",
    "test:db:users": "node scripts/test/bootstrap-local-auth-users.mjs",
    "test:db:fixtures": "node scripts/test/run-supabase-sql-suite.mjs --fixtures-only",
    "test:db:rls": "node scripts/test/run-supabase-sql-suite.mjs",
    "test:db:quick": "node scripts/test/run-supabase-sql-suite.mjs --skip-fixtures",
    "test:db": "node scripts/test/run-db-suite.mjs",
    "test:all": "npm run lint && npx tsc --noEmit --project tsconfig.json && npm run test:unit && npm run test:db && npm run test:backend",
    "test:ci": "npm run lint && npx tsc --noEmit --project tsconfig.json && npm run test:unit && npm run test:db && npm run test:backend",
    "ci:verify": "npm run test:ci"
  },
  "keywords": [],
  "author": {
    "name": "redrix-dev",
    "email": "redrixdevelopment@icloud.com"
  },
  "license": "MIT",
